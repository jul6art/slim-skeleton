{
    "type": "project",
    "name": "jul6art/slim-skeleton",
    "description": "Slim framework Back-office package",
    "license": "MIT",
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "fullpipe/twig-webpack-extension": "^3.0",
        "fzaninotto/faker": "^1.9",
        "illuminate/database": "^6.8",
        "illuminate/translation": "^6.8",
        "kanellov/slim-twig-flash": "^0.2.0",
        "monolog/monolog": "^2.0",
        "nette/finder": "^2.5",
        "php-di/php-di": "^6.0",
        "robmorgan/phinx": "^0.12.1",
        "slim/csrf": "^1.0",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.1",
        "slim/twig-view": "3.1.0",
        "swiftmailer/swiftmailer": "^6.2",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit",
        "cli": "php public/index.php"
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
